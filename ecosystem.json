{
  "apps": [{
    "name": "cribbage-counter",
    "script": "index.js",
    "log_date_format": "YYYY-MM-DD HH:mm:ss"
  }],
  "deploy" : {
    "stage" : {
      "user" : "deploy",
      "host" : "https://github.com",
      "ref"  : "origin/main",
      "repo" : "git@github.com:NicholasBrockman/cribbage-counter.git",
      "path" : "~/cribbage-counter",
      "post-deploy" : "npm i && npm run build"
    },
    "production" : {
      "user" : "deploy",
      "host" : "https://github.com",
      "ref"  : "origin/main",
      "repo" : "git@github.com:NicholasBrockman/cribbage-counter.git",
      "path" : "~/cribbage-counter",
      "post-deploy" : "npm i && npm run build"
    }
  }
}
